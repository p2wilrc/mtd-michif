{
  "name": "mtd-web-ui",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run server",
    "start:prod": "npm run server",
    "build": "ng build --prod --outputHashing=all",
    "build:prod": "ng build --prod --outputHashing=all",
    "build:ci": "ng build --prod --outputHashing=all",
    "minify": "uglifyjs projects/mtd/src/assets/js/mtd.js projects/mtd/src/assets/js/levenshtein.js projects/mtd/src/assets/js/editDistance.js projects/mtd/src/assets/js/utility.js projects/mtd/src/assets/js/deadSearch.js -o projects/mtd/src/assets/js/mtd-ui.min.js",
    "lint": "ng lint",
    "test": "ng lint && ng test --configuration=test",
    "watch": "ng test --configuration=test --browsers ChromeHeadless --watch --reporters dots",
    "e2e": "ng e2e",
    "e2e:ci": "ng e2e --webdriver-update false",
    "pree2e:ci": "./node_modules/protractor/bin/webdriver-manager clean && ./node_modules/protractor/bin/webdriver-manager update --versions.chrome=62.0.3202",
    "ci": "ng lint && ng test --configuration=test --browsers ChromeTravisCi --code-coverage",
    "format:write": "prettier projects/**/*.{ts,json,md,scss} --write",
    "format:test": "prettier projects/**/*.{ts,json,md,scss} --list-different",
    "release": "standard-version && git push --follow-tags origin master",
    "analyze": "npm run build:prod -- --stats-json && webpack-bundle-analyzer ./dist/mtd/stats-es2015.json",
    "server": "node server.js",
    "extract-translations": "ngx-translate-extract --input ./projects/mtd/src --output ./projects/mtd/src/assets/i18n/*.json --sort --format namespaced-json --null-as-default-value --marker _",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate && node .all-contributors-html.js",
    "contributors:check": "all-contributors check"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular-extensions/elements": "^11.0.1",
    "@angular-extensions/model": "^11.0.0",
    "@angular/animations": "~11.2.14",
    "@angular/cdk": "~11.2.13",
    "@angular/cli": "~11.2.19",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/forms": "~11.2.14",
    "@angular/material": "~11.2.13",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@angular/router": "~11.2.14",
    "@angular/service-worker": "~11.2.14",
    "@biesbjerg/ngx-translate-extract": "^7.0.2",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@fortawesome/angular-fontawesome": "^0.8.2",
    "@fortawesome/fontawesome-free": "^5.12.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@ngrx/effects": "^11.1.1",
    "@ngrx/entity": "^11.1.1",
    "@ngrx/router-store": "^11.1.1",
    "@ngrx/store": "^11.1.1",
    "@ngrx/store-devtools": "^11.1.1",
    "@ngx-translate/core": "^12.0.0",
    "@ngx-translate/http-loader": "^4.0.0",
    "bootstrap": "^4.4.1",
    "browser-detect": "^0.2.28",
    "jquery": "^1.9.1",
    "popper.js": "^1.16.1",
    "rxjs": "^6.6.7",
    "transliteration": "^2.1.11",
    "uuid": "^3.4.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.19",
    "@angular/compiler-cli": "~11.2.14",
    "@angular/language-service": "~11.2.14",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "@types/uuid": "^3.4.7",
    "all-contributors-cli": "^6.13.0",
    "codelyzer": "^6.0.0",
    "express": "^4.18.2",
    "husky": "^4.2.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-spec-reporter": "^0.0.32",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "standard-version": "^8.0.1",
    "ts-node": "~8.6.2",
    "tslib": "^1.14.1",
    "tslint": "~6.1.0",
    "typescript": "^4.1.6",
    "uglifyjs": "^2.4.11",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
